{
  "name": "ts-scientific-computing",
  "version": "1.0.0",
  "description": "TypeScript implementations of NumPy, Matplotlib, and Scikit-learn",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs": "typedoc src/index.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "typescript",
    "numpy",
    "matplotlib",
    "scikit-learn",
    "scientific-computing",
    "machine-learning",
    "data-science",
    "visualization"
  ],
  "author": "Scientific Computing TS Team",
  "license": "BSD-3-Clause",
  "dependencies": {
    "ml-matrix": "^6.11.0",
    "simple-statistics": "^7.8.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ts-scientific-computing.git"
  },
  "bugs": {
